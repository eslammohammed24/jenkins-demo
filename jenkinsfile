pipeline {
    agent any
    tools {
    maven "sprint-maven"
    
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh "mvn package"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    post {
    always {
      echo 'I will always execute this!'
    }
  }
  }  
}
